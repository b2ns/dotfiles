{
  "snippets.priority": 100,
  "snippets.loadFromExtensions": false,
  "snippets.snipmate.enable": false,
  "snippets.textmateSnippetsRoots": ["~/.config/Code/User/snippets"],
  "diagnostic.displayByAle": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "suggest.defaultSortMethod": "none",
  "colors.filetypes": ["*"],
  "outline.sortBy": "position",
  "vetur.ignoreProjectWarning": true,
  "cssmodules.camelCase": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "s": "open:vsplit",
    "E": "open:split",
    "<c-t>": "open:tab"
  },
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "typescriptreact",
    "javascriptreact",
    "javascript",
    "typescript"
  ]
}
